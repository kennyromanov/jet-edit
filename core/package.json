{
  "name": "jet-edit",
  "version": "1.1.0",
  "description": "An Advanced Document Editor",
  "keywords": ["editor", "document editor", "flext"],
  "type": "module",
  "main": "dist/index.es.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    },
    "./index.css": "./dist/index.css",
    "./fonts.css": "./dist/fonts.css"
  },
  "scripts": {
    "lint": "eslint",
    "shadcn": "bin/shadcn",
    "start": "bin/start",
    "build:types": "vue-tsc --noEmit false && tsc-alias",
    "build-only": "vite build",
    "build": "npm run build-only && npm run build:types",
    "prepare": "npm run build"
  },
  "files": [
    "dist/"
  ],
  "author": "Kenny Romanov",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@tailwindcss/cli": "^4.1.16",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.9.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.4.0",
    "sass-embedded": "^1.93.2",
    "tsc-alias": "^1.8.16",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vite-plugin-eslint": "^1.8.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.2"
  },
  "dependencies": {
    "@trustme24/flext": "^1.4.3",
    "@vueuse/core": "^14.0.0",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "lucide-vue-next": "^0.548.0",
    "mitt": "^3.0.1",
    "nanoid": "^5.1.6",
    "pinia": "^3.0.3",
    "reka-ui": "^2.6.0",
    "select": "^1.1.2",
    "shadcn": "^3.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "vue": "^3.5.22",
    "vue-edit": "^1.1.0",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3",
    "vue-sonner": "^2.0.9"
  }
}
